{
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "overrides": [
        {
            "files": [
                "*.ts"
            ],
            "rules": {
                "semi": [
                    "error",
                    "always"
                ],
                "quotes": [
                    "error",
                    "double"
                ]
            }
        }
    ]
}